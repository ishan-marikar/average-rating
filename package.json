{
  "version": "0.3.0",
  "name": "average-rating",
  "description": "Calculate average score and rating based on Wilson Score Equation",
  "homepage": "http://techpush.net",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/average-rating.git"
  },
  "author": "Dong Nguyen <ndaidong@gmail.com> (https://twitter.com/ndaidong)",
  "main": "./index.js",
  "engines": {
    "node": ">= 4.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/tape test/start.js | tap-spec",
    "coverage": "./node_modules/.bin/nyc tape test/start.js | tap-spec",
    "report": "npm run coverage && ./node_modules/.bin/nyc report --reporter=lcov",
    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "coveralls": "latest",
    "nyc": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "rating",
    "vote",
    "poll",
    "average",
    "score",
    "Wilson Score",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/average-rating/issues"
  },
  "directories": {
    "test": "test"
  }
}
